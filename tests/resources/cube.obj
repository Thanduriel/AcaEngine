# vertices only support 3d
v 0 0 0
v 1 0 0
v 0 1 0
v 0 0 1

# texture only support 2d
vt 0. 0.
vt 1. 0.
vt 0. 1.
vt 1. 1.

# normals only support 3d
vn 1. 0. 0.
vn 0. 1. 0.
vn 0. 0. -1.
vn 1. 1. 1.

# faces vertex/texture/normal
# tet_complete
f 0/0/0 3/3/0 1/1/0
f 0/0/1 2/2/1 3/3/1
f 0/0/2 1/1/2 2/2/2
f 3/3/3 1/1/3 2/2/3

# faces vertex//normal
# tet_normal
f 0//0 3//0 1//0
f 0//1 2//1 3//1
f 0//2 1//2 2//2
f 3//3 1//3 2//3

# faces vertex/texture
# tet_texture
f 0/0 3/3 1/1
f 0/0 2/2 3/3
f 0/0 1/1 2/2
f 3/3 1/1 2/2

# faces vertex
# tet
f 0 3 1
f 0 2 3
f 0 1 2
f 3 1 2


